    "version": "0.2.0",
    "configurations": [

        {
            "name": "root",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [""]
        },
        {
            "name": "workspace",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["workspace", "list", "--organization", "foo-org"]
        },
        {
            "name": "configure",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["configure", "create", "--profile",  "gitlab", "--team-token", "your-token", "--new-name", "new-gitlab"]
        },
        {
            "name": "configure-create",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["configure", "create"]
        },
        {
            "name": "devops-ga-awsappteam",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["workspace", "list",  "--profile", "bhp-app-migration", "--organization", "devops-ga-awsappteam", "--enabled", "true"]
        }
    ]
}